{
    "name": "zfury/cmf",
    "description": "Skeleton Application based on ZF2",
    "license": "MIT",
    "keywords": [
        "framework",
        "zf2",
        "zfury",
        "cmf"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.3.*",
        "doctrine/common": "2.4.*",
        "doctrine/doctrine-orm-module": "0.8.*",
        "bjyoungblood/bjy-authorize": "1.4.*",
        "facebook/php-sdk-v4": "4.0.*",
        "doctrine/migrations": "dev-master",
        "sundrique/doctrine-tools": ">=0.2",
        "zendframework/zendoauth": "2.0.*",
        "zfury/framework": "0.1.0",
        "beberlei/DoctrineExtensions": "v0.2-alpha"
    },
    "require-dev": {
        "mayflower/php-codebrowser": "1.1.1",
        "phpunit/phpunit": "4.3.*",
        "squizlabs/php_codesniffer": "2.2.0",
        "zendframework/zend-developer-tools": "dev-master",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-0": {
            "": "library/"
        }
    },
    "suggests": {
        "libav-tools": "to convert media files",
        "libavcodec-extra-53": "to convert media files"
    }
}
