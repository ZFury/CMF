{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.*",
        "doctrine/common": "2.4.*",
        "doctrine/doctrine-orm-module": "0.8.*",
        "bjyoungblood/bjy-authorize": "1.4.*",
        "facebook/php-sdk-v4": "4.0.*",
        "doctrine/migrations": "dev-master",
        "sundrique/doctrine-tools": ">=0.2",
        "zendframework/zendoauth": "2.0.*",
        "ripaclub/sphinxsearch": "0.7.0"
    },
    "require-dev": {
        "mayflower/php-codebrowser": "1.1.1",
        "phpunit/phpunit": "4.3.*",
        "squizlabs/php_codesniffer": "2.0.0RC1",
        "zendframework/zftool": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "": "library/"
        }
    },
    "suggest": {
        "libav-tools": "to convert media files",
        "libavcodec-extra-53": "to convert media files"
    }
}
